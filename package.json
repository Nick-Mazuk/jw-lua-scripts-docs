{
    "name": "sveltekit-test",
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "dev": "svelte-kit dev",
        "dev:open": "svelte-kit dev --open",
        "stork": "stork --build config.toml",
        "stork-prod": "bin/stork.bin --build config.toml",
        "prebuild": "./node_modules/.bin/esbuild cli/src/prebuild --outfile=cli/dist/prebuild.mjs --platform=node --target=node12.17 && node cli/dist/prebuild.mjs",
        "build": "svelte-kit build",
        "start": "svelte-kit start",
        "preview": "svelte-kit preview",
        "check-types": "tsc --noemit",
        "prettier": "prettier --write . --plugin-search-dir=. --quiet",
        "svelte-check": "svelte-check --ignore=cli/src/templates",
        "eslint": "eslint . --quiet",
        "lint": "npm run prettier && npm run check-types && npm run svelte-check && npm run eslint"
    },
    "devDependencies": {
        "@nick-mazuk/eslint-config": "0.8.11",
        "@nick-mazuk/ui-config": "0.10.61",
        "@sveltejs/adapter-vercel": "1.0.0-next.19",
        "@sveltejs/kit": "^1.0.0-next.107",
        "@types/fs-extra": "^9.0.9",
        "autoprefixer": "^10.2.5",
        "esbuild": "^0.12.0",
        "eslint": "^7.25.0",
        "fs-extra": "^10.0.0",
        "postcss": "^8.2.8",
        "postcss-load-config": "^3.0.1",
        "prettier": "^2.2.1",
        "prettier-plugin-svelte": "^2.2.0",
        "svelte": "^3.38.2",
        "svelte-check": "^1.5.2",
        "svelte-preprocess": "^4.7.3",
        "tailwindcss": "^2.1.2",
        "tslib": "^2.0.0",
        "typescript": "^4.2.4",
        "vite": "2.3.4"
    },
    "dependencies": {
        "@nick-mazuk/lib": "^0.8.54",
        "@nick-mazuk/ui-svelte": "0.14.6",
        "i": "^0.3.6",
        "jest": "^27.0.0",
        "stork-search": "^1.0.4"
    }
}
